{
  "name": "php-types",
  "displayName": "PHP Doc Types",
  "description": "PHP Doc Types Extended with generics, templates, inheritance and more",
  "icon": "res/icon.png",
  "version": "0.1.0",
  "publisher": "ErnstStavroBlofeld",
  "repository": {
    "url": "https://github.com/ErnstStavroBlofeld/php-types"
  },
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "phpdoc"
      }
    ],
    "grammars": [
      {
        "language": "phpdoc",
        "scopeName": "text.phpdoc",
        "injectTo": [
          "text.html.php"
        ],
        "path": "./syntaxes/php.tmLanguage.json"
      }
    ]
  }
}
